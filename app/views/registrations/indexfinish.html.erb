<%= render "shared/second-header" %>

<div class="registration-main">
  <div class="form-wrap">
    <div class='form-header'>
       <div class='form-header-text2'>
        <p>アカウントをお持ちでない方はこちら</p>
         <%= link_to '新規会員登録', new_user_registration_path, class:"new-regi"%>
       </div>
    </div>
  
     <div class="form-group-registration">
      <div class='form-text-wrap'>
        <div class="form-text-registration">
          <%= link_to 'メールアドレスでログイン', new_user_registration_path, class:"mail"%>
        </div>
      </div>
    </div>
    <div class="form-group-registration">
      <div class='form-text-wrap'>
        <div class="form-text-registration">
          <%= link_to 'Facebookでログイン', user_facebook_omniauth_authorize_path, method: :post, class:"facebook" %>
      </div>
      </div>    
    </div>
     <div class="form-group-registration">
      <div class='form-text-wrap'>
        <div class="form-text-registration">
          <%= link_to 'Googleでログイン', user_google_oauth2_omniauth_authorize_path, method: :post, class:"google" %>
        </div>
       </div>
    </div>
  </div>
</div>


<%= render "shared/second-footer" %>